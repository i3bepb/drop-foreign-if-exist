{
    "name": "i3bepb/laravel-drop-foreign-if-exists",
    "description": "Method drop foreign if exist for psql",
    "license":"MIT",
    "require": {
        "php": "^7.4 || ^8.0",
        "illuminate/support": "^7.0 || ^8.0 || ^9.0"
    },
    "require-dev": {
        "phpunit/phpunit": "^9.5",
        "orchestra/testbench": "^v6.0 || ^v7.0",
        "nunomaduro/collision": "^6.2 || ^5.0"
    },
    "autoload": {
        "psr-4": {
            "I3bepb\\DropForeignIfExists\\": "src/",
            "I3bepb\\DropForeignIfExists\\Tests\\": "tests/"
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "I3bepb\\DropForeignIfExists\\ServiceProvider"
            ]
        }
    }
}
